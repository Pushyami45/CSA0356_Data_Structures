#include<stdio.h>
int main ()
{
int n,i=1,fact=1;
printf("\nenter n value");
scanf("%d",& n);
while(i<=n)
{
fact=fact*i;
i++;
}
if(n>=1)
{
printf("%d",fact);
}
else if(n<0)
{
	printf("\nthe entered number is negative");
}
else
{
printf("\ncharacters are not allowed");
printf("\nenter the valid input\n");
}
return 0;
}
int fact(int n)
{
  if(n==0)  
    return(1);
return(n*fact(n-1));
}
