#include<stdio.h>    
int main()    
{    
 int f1,f2,f3,i,n;    
 printf("Enter the number of elements:");    
 scanf("%d",&n);
 f1=0;
 f2=1;  
 printf("%d\n",f1,f2);
 for(i=2;i<n;++i)  
 {   
  f3=f1+f2;        
  f1=f2;    
  f2=f3; 
  printf("%d\n",f3);   
 }  
  return 0;  
 }
 int Fibonacci(int n)
{
   if ( n == 0 )
      return 0;
   else if ( n == 1 )
      return 1;
   else
      return ( Fibonacci(n-1) + Fibonacci(n-2) );
} 
